{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2015",
    "strict": true,
    "composite": true,
    "declaration": true,
    "outDir": "../../dist/hsr-node-typescript",
    "strictNullChecks": true,
    "lib": [
      "es6"
    ],
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@no0dles/hsr-node/*": ["../../hsr-node/src/*"]
    },
    "tsBuildInfoFile": "../../dist/hsr-node-typescript/tsbuildinfo"
  },
  "references": [
    {
      "path": "../hsr-node/tsconfig.json"
    }
  ]
}
