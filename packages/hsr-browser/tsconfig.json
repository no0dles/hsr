{
  "compilerOptions": {
    "module": "ES2015",
    "target": "ES2015",
    "strict": true,
    "composite": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "strictNullChecks": true,
    "lib": [
      "dom",
      "es6"
    ],
    "rootDir": "src",
    "baseUrl": "src",
    "tsBuildInfoFile": "dist/tsbuildinfo"
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
